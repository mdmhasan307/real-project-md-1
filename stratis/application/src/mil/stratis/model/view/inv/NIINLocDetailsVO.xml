  <Variable
      Name="locIdStr"
      Kind="where"
      Type="java.lang.String">
  </Variable>
  <SQLQuery><![CDATA[
SELECT unique nf.NIIN, 
       nf.NIIN_ID, 
       nf.NOMENCLATURE,  
       nvl(nf.SHELF_LIFE_CODE,0) SHELF_LIFE_CODE,
       nl.NIIN_LOC_ID, 
       nl.qty,    
       nl.EXPIRATION_DATE EXPIRATION_DATE,
       l.LOCATION_LABEL, 
       l.LOCATION_ID,
       l.WAC_ID,    
       l.LOC_LEVEL, 
       l.BAY, 
       l.SIDE, 
       l.MECHANIZED_FLAG,
       l.SLOT
FROM NIIN_INFO nf, LOCATION l, NIIN_LOCATION nl
WHERE (nl.LOCATION_ID = l.LOCATION_ID) 
    AND nl.NIIN_ID = nf.NIIN_ID
    AND l.LOCATION_ID = nvl(:locIdStr,'0')
   ]]></SQLQuery>
