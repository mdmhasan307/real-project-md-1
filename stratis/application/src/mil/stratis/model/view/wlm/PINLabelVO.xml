   <DesignTime>
      <Attr Name="_codeGenFlag2" Value="Coll|VarAccess" />
      <Attr Name="_isExpertMode" Value="true" />
   </DesignTime>
   <Variable
      Name="pinStr"
      Kind="where"
      Type="java.lang.String" >
   </Variable>
   <SQLQuery><![CDATA[select l.mechanized_flag, l.location_label, nf.fsc, nf.niin, 
       nf.ui, nl.cc, i.document_number, i.suffix, p.pin, p.qty_picked, i.scn,
       u.username, to_char(nvl(p.time_of_pick, sysdate),'YYYYMMDD/HHMI') pin_date, w.wac_number, e.name work_station_name, nf.nomenclature
from issue i, picking p, users u, niin_location nl, equip e,
     niin_info nf, location l, wac w, packing_station ps, packing_consolidation pc
where (p.pin = :pinStr) and
      (p.scn = i.scn) and
      (p.modified_by = u.user_id) and
      (p.niin_loc_id = nl.niin_loc_id) and
      (i.niin_id = nf.niin_id) and
      (nl.location_id = l.location_id) and
      (l.wac_id = w.wac_id)  and
      (nl.niin_id = nf.niin_id) and
      (p.packing_consolidation_id = pc.packing_consolidation_id) and
      (pc.packing_station_id = ps.packing_station_id) and
      (ps.equipment_number = e.equipment_number)]]></SQLQuery>
