  <SQLQuery><![CDATA[SELECT Customer.ADDRESS_1, 
       Customer.CITY, 
       Customer.STATE, 
       Customer.ZIP_CODE, 
       Customer.AAC, 
       Customer.CUSTOMER_ID, 
       Customer.ADDRESS_2, 
       Customer.ROUTE_ID, 
       Customer.RESTRICT_SHIP, 
       Customer.NAME, 
       Customer.FLOOR_LOCATION_ID, 
       Customer.SUPPORTED, 
       Customer.SHIPPING_ROUTE_ID, 
       FloorLocation.FLOOR_LOCATION_ID AS FLOOR_LOCATION_ID1, 
       FloorLocation.FLOOR_LOCATION AS FLOOR_LOCATION1, 
       Route.ROUTE_ID AS ROUTE_ID1, 
       Route.ROUTE_NAME, 
       ShippingRoute.ROUTE_ID AS ROUTE_ID2, 
       ShippingRoute.ROUTE_NAME AS ROUTE_NAME1
FROM ((CUSTOMER Customer LEFT JOIN FLOOR_LOCATION FloorLocation ON Customer.FLOOR_LOCATION_ID=FloorLocation.FLOOR_LOCATION_ID) LEFT JOIN ROUTE Route ON Customer.ROUTE_ID=Route.ROUTE_ID) LEFT JOIN SHIPPING_ROUTE ShippingRoute ON Customer.SHIPPING_ROUTE_ID=ShippingRoute.ROUTE_ID]]></SQLQuery>
