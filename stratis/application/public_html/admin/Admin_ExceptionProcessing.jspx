                                                                               headerText="Qty">
                                                                        <af:outputText value="#{row.QTY}">
                                                                            <f:convertNumber groupingUsed="false"
                                                                                             pattern="#{bindings.RefusalBuffer1.formats.QTY}"/>
                                                                        </af:outputText>
                                                                    </af:column>
                                                                    <af:column sortProperty="QTY_REFUSED" sortable="false"
                                                                               headerText="Qty Refused">
                                                                        <af:outputText value="#{row['QTY_REFUSED']}">
                                                                            <f:convertNumber groupingUsed="false"
                                                                                             pattern="#{bindings.RefusalBuffer1.formats['QTY_REFUSED']}"/>
                                                                        </af:outputText>
                                                                    </af:column>
                                                                    <af:column sortProperty="ADVICE_CODE" sortable="false"
                                                                               headerText="Advice Code">
                                                                        <af:outputText value="#{row.ADVICE_CODE}"/>
                                                                    </af:column>
                                                                    <af:column sortProperty="ISSUE_TYPE" sortable="false"
                                                                               headerText="Issue Type">
                                                                        <af:outputText value="#{row.ISSUE_TYPE}"/>
                                                                    </af:column>
                                                                    <af:column sortProperty="CC" sortable="false"
                                                                               headerText="CC">
                                                                        <af:outputText value="#{row.CC}"/>
                                                                    </af:column>
                                                                </af:table>
                                                                <af:toolbar>
                                                                    <af:button text="Get Details"
                                                                               actionListener="#{ExceptionBacking.getRefusalDetails}"
                                                                               binding="#{ExceptionBacking.refusalDetailButton}"
                                                                               partialSubmit="false"/>
                                                                </af:toolbar>
                                                            </af:panelFormLayout>
                                                        </af:panelBox>
                                                        <af:switcher facetName="#{ExceptionBacking.refusalDetailState}">
                                                            <f:facet name="1">
                                                                <af:panelGroupLayout>
                                                                    <af:spacer height="10"/>
                                                                    <af:panelBox text="Refusal Details"
                                                                                 inlineStyle="text-align:center;"
                                                                                 showDisclosure="false">
                                                                        <af:spacer height="10"/>
                                                                        <af:table value="#{bindings.RefusalBuffer2.collectionModel}"
                                                                                  var="row"
                                                                                  rows="#{bindings.RefusalBuffer2.rangeSize}"
                                                                                  first="#{bindings.RefusalBuffer2.rangeStart}"
                                                                                  emptyText="#{bindings.RefusalBuffer2.viewable ? 'No rows yet.' : 'Access Denied.'}"
                                                                                  inlineStyle="text-align:left;"
                                                                                  autoHeightRows="5" width="100%">
                                                                            <af:column sortProperty="SCN" sortable="false" width="60"
                                                                                       headerText="SCN">
                                                                                <af:outputText value="#{row.SCN}"/>
                                                                            </af:column>
                                                                            <af:column sortProperty="SUFFIX" sortable="false" width="35"
                                                                                       headerText="#{bindings.RefusalBuffer2.labels.SUFFIX}">
                                                                                <af:outputText value="#{row.SUFFIX}">
                                                                                    <f:convertNumber groupingUsed="false"
                                                                                                     pattern="#{bindings.RefusalBuffer2.formats.SUFFIX}"/>
                                                                                </af:outputText>
                                                                            </af:column>
                                                                            <af:column sortProperty="DOCUMENT_NUMBER"
                                                                                       sortable="false"
                                                                                       headerText="Document Number">
                                                                                <af:outputText value="#{row['DOCUMENT_NUMBER']}"/>
                                                                            </af:column>
                                                                            <af:column sortProperty="NIIN" sortable="false" width="50"
                                                                                       headerText="NIIN">
                                                                                <af:outputText value="#{row.NIIN}"/>
                                                                            </af:column>
                                                                            <af:column sortProperty="Location_Label" width="50"
                                                                                       sortable="false" headerText="Location">
                                                                                <af:outputText value="#{row.Location_Label}"/>
                                                                            </af:column>
                                                                            <af:column sortProperty="ADVICE_CODE" width="50"
                                                                                       sortable="false"
                                                                                       headerText="Advice Code">
                                                                                <af:outputText value="#{row.ADVICE_CODE}"/>
                                                                            </af:column>
                                                                            <af:column sortProperty="ISSUE_TYPE" width="50"
                                                                                       sortable="false" headerText="Issue Type">
                                                                                <af:outputText value="#{row.ISSUE_TYPE}"/>
                                                                            </af:column>
                                                                            <af:column sortProperty="CC" sortable="false" width="50"
                                                                                       headerText="CC">
                                                                                <af:outputText value="#{row.CC}"/>
                                                                            </af:column>
                                                                            <af:column sortProperty="QTY" sortable="false" width="50"
                                                                                       headerText="Issue Qty">
                                                                                <af:outputText value="#{row.QTY}">
                                                                                    <f:convertNumber groupingUsed="false"
                                                                                                     pattern="#{bindings.RefusalBuffer2.formats.QTY}"/>
                                                                                </af:outputText>
                                                                            </af:column>
                                                                            <af:column sortProperty="QTY_REFUSED" width="50"
                                                                                       sortable="false"
                                                                                       headerText="Qty Refused">
                                                                                <af:outputText value="#{row['QTY_REFUSED']}">
                                                                                    <f:convertNumber groupingUsed="false"
                                                                                                     pattern="#{bindings.RefusalBuffer2.formats['QTY_REFUSED']}"/>
                                                                                </af:outputText>
                                                                            </af:column>
                                                                            <af:column sortProperty="QTY_PICKED" width="50"
                                                                                       sortable="false"
                                                                                       headerText="QTY TO PICK / QTY PICKED">
                                                                                <af:outputText value="#{row['QTY_PICKED']}"/>
                                                                            </af:column>
                                                                            <af:column sortProperty="QTY_ONHAND" width="50"
                                                                                       sortable="false"
                                                                                       headerText="NIIN QTY ONHAND">
                                                                                <af:outputText value="#{row.QTY_ONHAND}">
                                                                                    <f:convertNumber groupingUsed="false"
                                                                                                     pattern="#{bindings.RefusalBuffer2.formats.QTY_ONHAND}"/>
                                                                                </af:outputText>
                                                                            </af:column>
                                                                            <af:column sortProperty="QTY_PENDING_PICK"
                                                                                       sortable="false"
                                                                                       headerText="ONHAND QTY ALLOCATED">
                                                                                <af:outputText value="#{row.QTY_PENDING_PICK}">
                                                                                    <f:convertNumber groupingUsed="false"
                                                                                                     pattern="#{bindings.RefusalBuffer2.formats.QTY_PENDING_PICK}"/>
                                                                                </af:outputText>
                                                                            </af:column>
                                                                            <af:column sortProperty="STATUS" sortable="false" width="50"
                                                                                       headerText="#{bindings.RefusalBuffer2.labels.STATUS}">
                                                                                <af:outputText value="#{row.STATUS}"/>
                                                                            </af:column>
                                                                        </af:table>
                                                                        <af:panelFormLayout>
                                                                            <af:selectBooleanRadio group="refuseDetailRadio"
                                                                                                   text="Re-Enter all refused picks into Workload Manager to attempt creation of picking task."
                                                                                                   binding="#{ExceptionBacking.refuseDetailRadio1}"
                                                                                                   id="rd1"/>
                                                                            <af:selectBooleanRadio group="refuseDetailRadio"
                                                                                                   text="Approve submission for issue as Partial-Refusal, allowing completion of Pack and Shipment of picked quantity."
                                                                                                   binding="#{ExceptionBacking.refuseDetailRadio2}"
                                                                                                   id="rd2"/>
                                                                            <af:selectBooleanRadio group="refuseDetailRadio"
                                                                                                   text="Approve refusal of entire issue quantity."
                                                                                                   binding="#{ExceptionBacking.refuseDetailRadio3}"
                                                                                                   id="rd3"/>
                                                                            <af:selectBooleanRadio group="refuseDetailRadio"
                                                                                                   text="Leave refusal unresolved, retain in refusal List and close window."
                                                                                                   binding="#{ExceptionBacking.refuseDetailRadio4}"
                                                                                                   id="rd4"/>
                                                                            <af:spacer height="5"/>
                                                                            <af:commandButton text="Submit"
                                                                                              actionListener="#{ExceptionBacking.submitRefusalDetails}"
                                                                                              binding="#{ExceptionBacking.saveRefuseDetails}"
                                                                                              id="srd1" partialSubmit="false"/>
                                                                            <af:spacer height="5"/>
                                                                        </af:panelFormLayout>
                                                                    </af:panelBox>
                                                                </af:panelGroupLayout>
                                                            </f:facet>
                                                        </af:switcher>
                                                    </af:panelBox>
                                                </af:panelGroupLayout>
                                            </f:facet>
                                        </af:switcher>
                                    </af:panelGroupLayout>
                                    <f:subview id="wrap_footer">
                                        <jsp:include page="/FooterMain.jspx"/>
                                    </f:subview>
                                </af:panelGroupLayout>
                            </f:facet>
                            <f:facet name="start">
                                <f:subview id="wrap_side_bar">
                                    <jsp:include page="/SideBarMain.jspx"/>
                                </f:subview>
                            </f:facet>
                        </af:panelStretchLayout>
                    </af:form>
                </af:document>
            </f:facet>
        </af:switcher>
    </f:view>
</jsp:root>
